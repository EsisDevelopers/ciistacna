<% end %>

<section>
  <div class="page-header">
    <h1><%= @pagina.titulo %></h1>
  </div>
  <p>
    <%= raw(@pagina.descripcion) %>
  </p>
  <br>
  <br>
  <p>
    <div class="fb-like" data-href="http://ciistacna.com/inicios/paginas/<%= @pagina.id %>" data-send="false" data-width="700" data-show-faces="true"></div>
    <div class="fb-comments" data-href="http://ciistacna.com/inicios/paginas/<%= @pagina.id %>" data-num-posts="5" data-width="700"></div>
    <div class="fb-like-box" data-href="https://www.facebook.com/XIII.CIIS.Tacna.2012?ref=hl&amp;bookmark_t=page" data-width="700" data-show-faces="true" data-stream="false" data-header="false"></div>
